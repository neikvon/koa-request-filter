  "description": "request filter middleware for koa 2+",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava --verbose",
    "example": "DEBUG=koa-filter node example/index.js"
  },
  "keywords": [
    "middleware",
    "koa",
    "request",
    "filter",
    "cookie",
    "ip"
